                    "Students"
                ],
                "summary": "Get list of students",
                "description": "Returns paginated list of students",
                "operationId": "getStudentsList",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Number of items per page",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 10
                        }
                    },
                    {
                        "name": "_",
                        "in": "query",
                        "description": "Cache-busting timestamp (optional)",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "example": 1763281133096
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Records retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
